%include "std.hz"

// Since Hazardous is stack based, you can perform stack operations directly.
// There are only 3 operations: push, pop (or drop) and call

proc sum(x: u32, y: u32) -> u32 {
    return x + y;
}

proc main(argc: i32, argv: u8**) -> i32 {
    push 10;
    push 5;
    call sum 2;

    push "Result is: %d\n";
    call printf 2;
    drop;

    return 0;
}

